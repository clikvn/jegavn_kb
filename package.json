{
  "name": "jega-kb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:api": "node src/dev-api-server.js",
    "dev": "concurrently \"npm run start:api\" \"npm run start\"",
    "dev:api": "cross-env NODE_ENV=development npm run start:api",
    "dev:frontend": "cross-env NODE_ENV=development npm run start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf build .docusaurus node_modules/.cache",
    "clean:all": "rm -rf build .docusaurus node_modules package-lock.json yarn.lock",
    "install:clean": "npm run clean:all && npm install",
    "health": "curl -f http://localhost:3001/api/health || echo 'API server not running'"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.0",
    "@docusaurus/preset-classic": "3.8.0",
    "@docusaurus/theme-classic": "3.8.0",
    "@docusaurus/theme-common": "3.8.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/genai": "^1.7.0",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "google-auth-library": "^9.15.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.0",
    "@docusaurus/types": "3.8.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "typescript": "~5.7.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "description": "JEGA Knowledge Base with AI Chatbot - Interior design software documentation and support",
  "keywords": [
    "jega",
    "aihouse",
    "interior-design",
    "documentation",
    "chatbot",
    "ai",
    "vertex-ai",
    "gemini"
  ],
  "author": "JEGA Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jega/knowledge-base.git"
  },
  "bugs": {
    "url": "https://github.com/jega/knowledge-base/issues"
  },
  "homepage": "https://jegavn-kb.vercel.app"
}
